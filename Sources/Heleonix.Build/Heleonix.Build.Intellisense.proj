    <Hxb-System-OpenCoverConsoleExe Include="0"/>
    <Hxb-System-ReportGeneratorExe Include="0"/>
    <Hxb-System-NugetExe Include="0"/>
    <Hxb-System-GitExe Include="0"/>
    <Hxb-System-SvnExe Include="0"/>
  </ItemGroup>
