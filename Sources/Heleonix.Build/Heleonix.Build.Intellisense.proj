      <Hxb-OpenCover-Out-VisitedClasses>0</Hxb-OpenCover-Out-VisitedClasses>
      <Hxb-OpenCover-Out-TotalMethods>0</Hxb-OpenCover-Out-TotalMethods>
      <Hxb-OpenCover-Out-VisitedMethods>0</Hxb-OpenCover-Out-VisitedMethods>
      <Hxb-OpenCover-Out-MinCyclomaticComplexity>0</Hxb-OpenCover-Out-MinCyclomaticComplexity>
      <Hxb-OpenCover-Out-MaxCyclomaticComplexity>0</Hxb-OpenCover-Out-MaxCyclomaticComplexity>
      <Hxb-OpenCover-Out-ClassCoverage>0</Hxb-OpenCover-Out-ClassCoverage>
      <Hxb-OpenCover-Out-MethodCoverage>0</Hxb-OpenCover-Out-MethodCoverage>
      <Hxb-OpenCover-Out-LineCoverage>0</Hxb-OpenCover-Out-LineCoverage>
      <Hxb-OpenCover-Out-BranchCoverage>0</Hxb-OpenCover-Out-BranchCoverage>
