    <ItemGroup>
      <Hxb-Rebuild-In-Delete Include="0"/>
      <Hxb-Rebuild-In-Clean-Dirs Include="0"/>
      <Hxb-Rebuild-In-Delete-Dirs Include="0"/>
      <Hxb-Rebuild-In-Snk Include="0"/>
      <Hxb-Rebuild-Out-Outputs Include="0"/>
      <Hxb-Rebuild-Out-Outputs-Tests Include="0"/>
    </ItemGroup>
  </Target>
  <!--Endregion-->

  <!--Region Target: Hxb-FxCop-->
  <Target Name="Hxb-FxCop">
    <PropertyGroup>
      <Hxb-FxCop-In-TargetsTypes></Hxb-FxCop-In-TargetsTypes>
      <Hxb-FxCop-In-FailOn>CriticalErrors</Hxb-FxCop-In-FailOn>
      <Hxb-FxCop-Out-CriticalErrors>0</Hxb-FxCop-Out-CriticalErrors>
      <Hxb-FxCop-Out-Errors>0</Hxb-FxCop-Out-Errors>
      <Hxb-FxCop-Out-CriticalWarnings>0</Hxb-FxCop-Out-CriticalWarnings>
      <Hxb-FxCop-Out-Warnings>0</Hxb-FxCop-Out-Warnings>
      <Hxb-FxCop-Out-Informational>0</Hxb-FxCop-Out-Informational>
    </PropertyGroup>

    <ItemGroup>
      <Hxb-FxCop-In-AnalysisResult Include="0"/>
      <Hxb-FxCop-In-Rules-FilesDirs Include="0"/>
      <Hxb-FxCop-In-Project Include="0"/>
      <Hxb-FxCop-In-Targets-FilesDirs Include="0"/>
      <Hxb-FxCop-In-AnalysisResultsXsl Include="0"/>
      <Hxb-FxCop-In-Dependencies-Dirs Include="0"/>
      <Hxb-FxCop-In-Ruleset Include="0"/>
      <Hxb-FxCop-In-Dictionary Include="0"/>
    </ItemGroup>
  </Target>
  <!--Endregion-->

  <!--Region Target: Hxb-NugetDeploy-->
  <Target Name="Hxb-NugetDeploy">
    <PropertyGroup>
      <Hxb-NugetDeploy-In-ApiKey></Hxb-NugetDeploy-In-ApiKey>
      <Hxb-NugetDeploy-In-IncludeReferencedProjects>true</Hxb-NugetDeploy-In-IncludeReferencedProjects>
      <Hxb-NugetDeploy-In-ExcludeEmptyDirectories>true</Hxb-NugetDeploy-In-ExcludeEmptyDirectories>
    </PropertyGroup>

    <ItemGroup>
      <Hxb-NugetDeploy-In-Nuspecs Include="0"/>
      <Hxb-NugetDeploy-In-Projects Include="0"/>
      <Hxb-NugetDeploy-In-PackagesDir Include="0"/>
      <Hxb-NugetDeploy-In-Config Include="0"/>
      <Hxb-NugetDeploy-In-Source Include="0"/>
      <Hxb-NugetDeploy-Out-Packages Include="0"/>
    </ItemGroup>
  </Target>
  <!--Endregion-->

  <!--Region Target: Hxb-NugetRestore-->
  <Target Name="Hxb-NugetRestore">
    <PropertyGroup>
      <Hxb-NugetRestore-In-MSBuildVersion>14</Hxb-NugetRestore-In-MSBuildVersion>
      <Hxb-NugetRestore-In-NoCache>true</Hxb-NugetRestore-In-NoCache>
    </PropertyGroup>

    <ItemGroup>
      <Hxb-NugetRestore-In-Sources Include="0"/>
      <Hxb-NugetRestore-In-Packages-Dir Include="0"/>
    </ItemGroup>
  </Target>
  <!--Endregion-->

  <!--Region Target: Hxb-NUnit-->
  <Target Name="Hxb-NUnit">
    <PropertyGroup>
      <Hxb-NUnit-In-TestsParameters></Hxb-NUnit-In-TestsParameters>
      <Hxb-NUnit-In-TestsFilter></Hxb-NUnit-In-TestsFilter>
      <Hxb-NUnit-In-AgentsNumber>1</Hxb-NUnit-In-AgentsNumber>
      <Hxb-NUnit-In-StopOnErrorOrFailedTest>false</Hxb-NUnit-In-StopOnErrorOrFailedTest>
      <Hxb-NUnit-In-UseTeamCityServiceMessages>false</Hxb-NUnit-In-UseTeamCityServiceMessages>
      <Hxb-NUnit-In-Framework></Hxb-NUnit-In-Framework>
      <Hxb-NUnit-In-ProcessIsolation>Separate</Hxb-NUnit-In-ProcessIsolation>
      <Hxb-NUnit-In-DomainIsolation>Multiple</Hxb-NUnit-In-DomainIsolation>
      <Hxb-NUnit-In-ShadowCopy>false</Hxb-NUnit-In-ShadowCopy>
      <Hxb-NUnit-In-FailOnFailedTests>false</Hxb-NUnit-In-FailOnFailedTests>
      <Hxb-NUnit-Out-TestCases>0</Hxb-NUnit-Out-TestCases>
      <Hxb-NUnit-Out-Total>0</Hxb-NUnit-Out-Total>
      <Hxb-NUnit-Out-Passed>0</Hxb-NUnit-Out-Passed>
      <Hxb-NUnit-Out-Failed>0</Hxb-NUnit-Out-Failed>
      <Hxb-NUnit-Out-Inconclusive>0</Hxb-NUnit-Out-Inconclusive>
      <Hxb-NUnit-Out-Skipped>0</Hxb-NUnit-Out-Skipped>
      <Hxb-NUnit-Out-Asserts>0</Hxb-NUnit-Out-Asserts>
      <Hxb-NUnit-Out-StartTime>2015-01-30T21:32:52</Hxb-NUnit-Out-StartTime>
      <Hxb-NUnit-Out-EndTime>2015-01-30T21:32:52</Hxb-NUnit-Out-EndTime>
      <Hxb-NUnit-Out-Duration>0</Hxb-NUnit-Out-Duration>
    </PropertyGroup>

    <ItemGroup>
      <Hxb-NUnit-In-NUnitProjectOrTests Include="0"/>
      <Hxb-NUnit-In-TestsResult Include="0"/>
      <Hxb-NUnit-In-TestsList Include="0"/>
      <Hxb-NUnit-In-TestsOutput Include="0"/>
      <Hxb-NUnit-In-ErrorsOutput Include="0"/>
    </ItemGroup>
  </Target>
