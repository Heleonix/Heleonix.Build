    <Hxb-System-CI>Jenkins</Hxb-System-CI>
    <Hxb-System-Workspace-Dir></Hxb-System-Workspace-Dir>
  </PropertyGroup>

  <ItemGroup>
    <Hxb-System-FxCopCmd Include="0"/>
