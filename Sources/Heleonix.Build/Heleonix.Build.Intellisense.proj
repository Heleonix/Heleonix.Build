  <!--Region Setup-->
  <PropertyGroup>
    <Hxb-Setup-Sln-Flows></Hxb-Setup-Sln-Flows>
  </PropertyGroup>
  <!--Endregion-->

  <!--Region Build-->
  <PropertyGroup>
    <Hxb-Build-VCS>Git</Hxb-Build-VCS>
    <Hxb-Build-Version></Hxb-Build-Version>
    <Hxb-Build-Version-Informational></Hxb-Build-Version-Informational>
    <Hxb-Build-Reports-Dir></Hxb-Build-Reports-Dir>
    <Hxb-Build-Deployments-Dir></Hxb-Build-Deployments-Dir>
    <Hxb-Build-Sln-Dir></Hxb-Build-Sln-Dir>
    <Hxb-Build-Sln-File></Hxb-Build-Sln-File>
    <Hxb-Build-Verbosity></Hxb-Build-Verbosity>
  </PropertyGroup>

  <ItemGroup>
    <Hxb-Build-Sln Include="0"/>
  </ItemGroup>
  <!--Endregion-->

  <!--Region Target: Hxb-Main-->
  <Target Name="Hxb-Main"/>
  <!--Endregion-->

  <!--Region Target: $(Hxb-In-Flow)-->
  <Target Name="$(Hxb-In-Flow)"/>
  <!--Endregion-->

  <!--Region Target: Hxb-Initialize-->
  <Target Name="Hxb-Initialize"/>
  <!--Endregion-->

  <!--Region Target: Hxb-Finalize-->
  <Target Name="Hxb-Finalize"/>
  <!--Endregion-->

  <!--Region Target: Hxb-OnError-->
  <Target Name="Hxb-OnError"/>
  <!--Endregion-->

  <!--Region Target: Hxb-Rebuild-->
  <Target Name="Hxb-Rebuild">
    <PropertyGroup>
      <Hxb-Rebuild-In-Outputs-RegEx></Hxb-Rebuild-In-Outputs-RegEx>
      <Hxb-Rebuild-In-Outputs-Tests-RegEx></Hxb-Rebuild-In-Outputs-Tests-RegEx>
